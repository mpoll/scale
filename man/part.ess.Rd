function (log.p.wei, p.num) 
{
    normalised <- part.normal(log.p.wei)
    list(log.p.wei = normalised$log.p.wei, p.wei = normalised$p.wei, 
        ess = 1/(p.num * sum(normalised$p.wei^2)))
  }
}
